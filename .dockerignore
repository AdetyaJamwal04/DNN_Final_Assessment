venv/
env/
ENV/
.venv

# IDE
.vscode/
.idea/
*.swp
*.swo

# Git
.git/
.gitignore
.gitattributes

# Documentation
*.md
docs/

# Tests
tests/
.pytest_cache/
.coverage

# CI/CD
.github/
.gitlab-ci.yml

